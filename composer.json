{
    "name": "onurure/omnipay-iyzico",
    "description": "Iyzico for omnipay 3 ",
    "keywords": [
        "omnipay",
        "iyzico"
    ],
    "homepage": "https://github.com/onurure/omnipay-iyzico",
    "license": "MIT",
    "authors": [
        {
            "name": "onur Ã¼re",
            "email": "ure.onur@gmail.com",
            "homepage": "https://github.com/onurure"
        }
    ],
    "require": {
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3",
        "phpro/grumphp": "^0.14"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\Iyzico\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Iyzico\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
